    "@react-native-segmented-control/segmented-control": "2.4.1",
    "@react-navigation/elements": "^1.3.21",
    "@react-navigation/native": "^6.0.2",
    "@shopify/flash-list": "1.6.3",
    "@shopify/restyle": "^2.4.2",
    "expo": "~50.0.11",
    "expo-clipboard": "~5.0.1",
    "expo-font": "~11.10.2",
    "expo-haptics": "~12.8.1",
